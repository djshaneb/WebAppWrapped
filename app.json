{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": [
      "bolt-expo-nativewind",
      "com.googleusercontent.apps.655876057326-34nnmbkgd077bjlsqvba0tke25mlkhm3"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.weddingwin.app",
      "associatedDomains": [
        "applinks:weddingwin.ca",
        "applinks:www.weddingwin.ca",
        "applinks:weddingwin.ca",
        "applinks:www.weddingwin.ca"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.weddingwin.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "weddingwin.ca",
              "pathPrefix": "/oauth-callback"
            },
            {
              "scheme": "https",
              "host": "www.weddingwin.ca",
              "pathPrefix": "/oauth-callback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "11db546d-12cb-4fd8-8423-ac14b508f682"
      }
    }
  }
}
